		<response>
			<body>
<![CDATA[
{ "actors": [
		{ "id": 1000, "first_name": "_Jack", "surname": "Smith\t" },
		{ "id": 1001, "first_name": "_Jack", "surname": "\"Johnson\"" }
	]
}]]>
			</body>
		</response>
	</step>
	<teardown>
		<sql>DELETE FROM actor where actor_id between 1000 and 1100</sql>
