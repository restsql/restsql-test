	xsi:schemaLocation="http://restsql.org/schema ServiceTestCaseDefinition.xsd ">
	<setup>
		<sql> <![CDATA[ INSERT INTO actor (actor_id,first_name,last_name) VALUES (1000,'_Jack','Smith	') ]]> </sql>
